col = 8
row = 8
i = 0
j = 0
while i < row:
    while j < col:
        if i == j:
            print("* ", end="")
        elif i + j == col - 1:
            print("* ", end="")
        elif i == 3 or i == 4:
            print("* ", end="")
        elif j == 3 or j == 4:
            print("* ", end="")
        else:
            print("  ", end="")
        j += 1
    i += 1
    j = 0
    print()
